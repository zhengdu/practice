class Solution:
    # @param {integer} x
    # @return {boolean}
    def isPalindrome(self, x):
        if x < 0:
            return False #pal =  -1 * int(s)
        else: 
            s = str(abs(x))
            s = s[::-1]
            pal =  int(s)

        if pal == x:
            return  True
        else:
          return False  



tt  = Solution()
print tt.isPalindrome(100)    		
